<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <servlet>
        <servlet-name>s1</servlet-name>
        <servlet-class>formServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>s1</servlet-name>
        <url-pattern>/form</url-pattern>
    </servlet-mapping>

    <filter>
        <filter-name>f2</filter-name>
        <filter-class>AuthFilter</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>f2</filter-name>
        <url-pattern>/auth</url-pattern>
    </filter-mapping>
    <filter>
        <filter-name>f5</filter-name>
        <filter-class>DateFilter</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>f5</filter-name>
        <url-pattern>/auth</url-pattern>
    </filter-mapping>

    <servlet>
        <servlet-name>s2</servlet-name>
        <servlet-class>reservationServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>s2</servlet-name>
        <url-pattern>/auth</url-pattern>
    </servlet-mapping>

    <filter>
        <filter-name>f3</filter-name>
        <filter-class>nationalCodeFilter</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>f3</filter-name>
        <url-pattern>/nationalCode</url-pattern>
    </filter-mapping>

    <servlet>
        <servlet-name>s3</servlet-name>
        <servlet-class>showInformationServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>s3</servlet-name>
        <url-pattern>/nationalCode</url-pattern>
    </servlet-mapping>


    <filter>
        <filter-name>f4</filter-name>
        <filter-class>reserveCodeFilter</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>f4</filter-name>
        <url-pattern>/cancel</url-pattern>
    </filter-mapping>

    <servlet>
        <servlet-name>s4</servlet-name>
        <servlet-class>CancelServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>s4</servlet-name>
        <url-pattern>/cancel</url-pattern>
    </servlet-mapping>

    <filter>
        <filter-name>f6</filter-name>
        <filter-class>reserveCodeFilter</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>f6</filter-name>
        <url-pattern>/change</url-pattern>
    </filter-mapping>

    <servlet>
        <servlet-name>s5</servlet-name>
        <servlet-class>ChangeServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>s5</servlet-name>
        <url-pattern>/change</url-pattern>
    </servlet-mapping>









</web-app>